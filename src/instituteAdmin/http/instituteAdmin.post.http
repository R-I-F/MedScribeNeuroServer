# You need to login first and include the JWT token in the Authorization header
POST http://localhost:3001/instituteAdmin
Content-Type: application/json
Authorization: Bearer ...

{
  "email": "xxx@gmail.com",
  "password": "xxx",
  "fullName": "Institute Admin User",
  "phoneNum": "01090650946",
  "approved": true
}

